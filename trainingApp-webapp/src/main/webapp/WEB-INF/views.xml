<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans 
             http://www.springframework.org/schema/beans/spring-beans-3.1.xsd"
	default-lazy-init="true">
	
	<bean id="subscribeForm" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/WEB-INF/jsp/subscribeForm.jsp"></property>
    </bean>
    
    <bean id="subscribeSuccess" class="org.springframework.web.servlet.view.JstlView">
        <property name="url" value="/WEB-INF/jsp/subscribeSuccess.jsp"></property>
    </bean>

</beans>